<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6" />
    </startup>

<system.serviceModel>
  <services>
    <service behaviorConfiguration="BookstoreServer.IBookServiceImplB"
      name="BookstoreServer.BookServiceImpl">
      <endpoint address="" binding="netTcpBinding" bindingConfiguration=""
        contract="BookstoreServer.IBookServiceImpl">
        <identity>
          <dns value="localhost" />
        </identity>
      </endpoint>
      <host>
        <baseAddresses>
          <add baseAddress="net.tcp://localhost:4567/BookService" />
        </baseAddresses>
      </host>
    </service>
  </services>
  <behaviors>
    <serviceBehaviors>
      <behavior name="BookstoreServer.IBookServiceImplB">
        <serviceMetadata httpGetEnabled="false" />
        <serviceDebug includeExceptionDetailInFaults="false" />
      </behavior>
    </serviceBehaviors>
  </behaviors>
</system.serviceModel>
</configuration>
